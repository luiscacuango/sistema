	<field name="PRODUCTO_INVENTARIO" class="java.lang.Integer"/>
	<field name="CANTIDAD_INVENTARIO" class="java.math.BigDecimal"/>
	<field name="PRODUCTO_STOCK" class="java.lang.Integer"/>
